<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/percetage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/progressExpensesBar"
        android:layout_marginRight="5dp"
        android:layout_weight="0"
        android:textAppearance="@style/ProgressNumberBudgets" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/percetage"
        android:layout_toRightOf="@+id/percetage"
        android:text="@string/executed"
        android:textAppearance="@style/ProgressNumberBudgets" />

    <ProgressBar
        android:id="@+id/progressExpensesBar"
        style="@android:style/Widget.ProgressBar.Horizontal"
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:indeterminate="false"
        android:indeterminateDrawable="@drawable/custom_expenses_progress"
        android:progress="50"
        android:progressBackgroundTint="@color/white"
        android:progressDrawable="@drawable/custom_expenses_progress"
        android:visibility="visible"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/expense"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="false"
        android:layout_below="@+id/percetage"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_weight="0"
        android:textAppearance="@style/ProgressNumberBudgets" />

</merge>